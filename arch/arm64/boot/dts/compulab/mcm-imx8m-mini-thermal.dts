#include "mcm-imx8m-mini.dts"

/ {
	model = "CompuLab MCM i.MX8MM board (thermal)";
	compatible = "cpl,mcm-imx8m-mini", "fsl,imx8mm";
};

&cpu_alert0 {
	temperature = <125000>;
};

